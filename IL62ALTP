LATLIST  DS    0D                                                       04150000
*IHXPCPIC                                                               04160000
         DC    CL64'IHXPCPIC'                TPNAME                     04170000
         DC    F'1'                          ICVT RESIDENT              04180000
         DC    A(#HXPCPIC-ICVT)              ICVT OFFSET                04190000
         DC    CL8' '                        NO LOAD MODULE NAME        04200000
         DC    CL4'PROC'                     RUNS AS A PROC             04210000
         DC    3F'0'                         EXPANSION                  04220000
*HXPCPIC                                                                04230000
         DC    CL64'HXPCPIC'                 TPNAME                     04240000
         DC    F'1'                          ICVT RESIDENT              04250000
         DC    A(#HXPCPIC-ICVT)              ICVT OFFSET                04260000
         DC    CL8' '                        NO LOAD MODULE NAME        04270000
         DC    CL4'PROC'                     RUNS AS A PROC             04280000
         DC    3F'0'                         EXPANSION                  04290000
*SHXPCPIC                                                               04300000
         DC    CL64'SHXPCPIC'                TPNAME                     04310000
         DC    F'1'                          ICVT RESIDENT              04320000
         DC    A(#HXPCPIC-ICVT)              ICVT OFFSET                04330000
         DC    CL8' '                        NO LOAD MODULE NAME        04340000
         DC    CL4'VTAM'                     RUNS AS A VTAM USER        04350000
         DC    3F'0'                         EXPANSION                  04360000
*IL62XKIL                                                               04370000
         DC    CL64'IL62XKIL'                TPNAME                     04380000
         DC    F'2'                          IVTAMCVT RESIDENT          04390000
         DC    A(IL6@XKIL-IVTAMCVT)          IVTAMCVT OFFSET            04400000
         DC    CL8' '                        NO LOAD MODULE NAME        04410000
         DC    CL4'PROC'                     RUNS AS A PROC             04420000
         DC    3F'0'                         EXPANSION                  04430000
*CNOS                                                                   04440000
         DC    X'06F1',CL62' '               TPNAME                     04450000
         DC    F'2'                          IVTAMCVT RESIDENT          04460000
         DC    A(IL6@XCNS-IVTAMCVT)          IVTAMCVT OFFSET            04470000
         DC    CL8' '                        NO LOAD MODULE NAME        04480000
         DC    CL4'PROC'                     RUNS AS A PROC             04490000
         DC    3F'0'                         EXPANSION                  04500000
*IL62XCNS                                                               04510000
         DC    CL64'IL62XCNS'                TPNAME                     04520000
         DC    F'2'                          IVTAMCVT RESIDENT          04530000
         DC    A(IL6@XCNS-IVTAMCVT)          IVTAMCVT OFFSET            04540000
         DC    CL8' '                        NO LOAD MODULE NAME        04550000
         DC    CL4'PROC'                     RUNS AS A PROC             04560000
         DC    3F'0'                         EXPANSION                  04570000
*INTTOINT                                                               04580000
         DC    CL64'INTTOINT'                TPNAME                     04590000
         DC    F'0'                          LOAD MODULE                04600000
         DC    A(0)                          NO OFFSET                  04610000
         DC    CL8'INTTOINT'                 LOAD MODULE NAME           04620000
         DC    CL4'VTAM'                     RUNS AS A VTAM USER        04630000
         DC    3F'0'                         EXPANSION                  04640000
*LAST ENTRY (DUMMY ENTRY)                                               04650000
         DC    CL64' '                                                  04660000
         DC    F'-1'                         END OF ENTRIES             04670000
         DC    A(0)                                                     04680000
         DC    CL8' '                                                   04690000
         DC    CL4' '                                                   04700000
         DC    3F'0'                                                    04710000
                                                                        04720000
         PRINT NOGEN                                                    04730000
         ISTDBIND ,               VTAM BIND IMAGE                       04740000
         IHAPSA ,                 MVS PREFIXED SAVE AREA                04750000
         IKJTCB ,                 MVS TASK CONTROL BLOCK                04760000
         TRACODES ,               INTEGRATOR TRACE CODE EQUATES         04770000
