#03      DC    CL16'SYSTEMS         ',A(TRC_SYSTEMS)                    00350000
#04      DC    CL16'TCP             ',A(TRC_TCP)                        00360000
#0D      DC    CL16'DISPATCH        ',A(TRC_DISP)                       00370000
#0E      DC    CL16'TASKMGR         ',A(TRC_TASK)                       00380000
#0F      DC    CL16'RESMGR          ',A(TRC_RESMGR)                     00390000
