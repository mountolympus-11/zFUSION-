#11      DC    CL16'OBJECT          ',A(TRC_OBJECT)                     00410000
#12      DC    CL16'SPACE           ',A(TRC_SPACE)                      00420000
#20      DC    CL16'NAVIGATION      ',A(TRC_NAV)                        00430000
#30      DC    CL16'XPORT           ',A(TRC_XPORT)                      00440000
