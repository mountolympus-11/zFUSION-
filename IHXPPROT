****************************************************************        02920000
*                                                                       02930000
* XPORT HEADER FORMAT: COMMON                                           02940000
*                                                                       02950000
*   The following definitions describe the format of the                02960000
*   common xport header.  This header is used internally                02970000
*   by the host xport component.  When requests are received,           02980000
*   the specified header is mapping into common header format.          02990000
*   On outbound transmissions,  the common header format is             03000000
*   mapped into the required format for the client.                     03010000
*                                                                       03020000
****************************************************************        03030000
                                                                        03040000
COMMON   $XPTHDR TYPE=HDR,BASE=XHDRCOMM,HDRLEN=XHDCLN                   03050000
                                                                        03060000
         $XPTHDR PRIMARY,XHDCAPPL,FLDTYPE=BIN                           03070000
         $XPTHDR LSESSID,XHDCLSES,FLDTYPE=BIN                           03080000
                                                                        03090000
         $XPTHDR ORG_HOST,XHDCORIG,FLDTYPE=BIN,                        +03100000
               VALUE=XHDCO_HO,OPTS=MUTEXCL+FIXVALUE                     03110000
         $XPTHDR ORG_PC,XHDCORIG,FLDTYPE=BIN,                          +03120000
               VALUE=XHDCO_PC,OPTS=MUTEXCL+FIXVALUE                     03130000
                                                                        03140000
         $XPTHDR NEWSESS,XHDCFUNC,FLDTYPE=CHAR,                        +03150000
               VALUE=XHDCF_NS,OPTS=MUTEXCL+FIXVALUE                     03160000
         $XPTHDR KILLSESS,XHDCFUNC,FLDTYPE=CHAR,                       +03170000
               VALUE=XHDCF_KS,OPTS=MUTEXCL+FIXVALUE                     03180000
         $XPTHDR CONVERSE,XHDCFUNC,FLDTYPE=CHAR,                       +03190000
               VALUE=XHDCF_CV,OPTS=MUTEXCL+FIXVALUE                     03200000
         $XPTHDR AVAIL,XHDCFUNC,FLDTYPE=CHAR,                          +03210000
               VALUE=XHDCF_AV,OPTS=MUTEXCL+FIXVALUE                     03220000
         $XPTHDR DROP,XHDCFUNC,FLDTYPE=CHAR,                           +03230000
               VALUE=XHDCF_DR,OPTS=MUTEXCL+FIXVALUE                     03240000
         $XPTHDR PING,XHDCFUNC,FLDTYPE=CHAR,                           +03250000
               VALUE=XHDCF_PN,OPTS=MUTEXCL+FIXVALUE                     03260000
                                                                        03270000
         $XPTHDR RETCODE,XHDCRC,FLDTYPE=INT                             03280000
         $XPTHDR RSNCODE,XHDCRSN,FLDTYPE=INT                            03290000
         $XPTHDR SEQNUM,XHDCSEQ,FLDTYPE=INT                             03300000
         $XPTHDR XMITLEN,XHDCXMIT,FLDTYPE=INT                           03310000
         $XPTHDR PHYSREC,XHDCRECP,FLDTYPE=INT                           03320000
         $XPTHDR LOGREC,XHDCRECL,FLDTYPE=INT                            03330000
                                                                        03340000
         $XPTHDR SQUISH,XHDCSQSH,FLDTYPE=BOOL,VALUE=TRUE                03350000
         $XPTHDR ENHCLOAK,XHDCXCLK,FLDTYPE=BOOL,VALUE=TRUE              03360000
                                                                        03370000
         $XPTHDR ENHSQ_L0,XHDCXSQH,FLDTYPE=BIN,                        +03380000
               VALUE=XHDCXLV0,OPTS=MUTEXCL+FIXVALUE                     03390000
         $XPTHDR ENHSQ_L3,XHDCXSQH,FLDTYPE=BIN,                        +03400000
               VALUE=XHDCXLV3,OPTS=MUTEXCL+FIXVALUE                     03410000
         $XPTHDR ENHSQ_L1,XHDCXSQH,FLDTYPE=BIN,                        +03420000
               VALUE=XHDCXLV1,OPTS=MUTEXCL+FIXVALUE                     03430000
         $XPTHDR ENHSQ_L2,XHDCXSQH,FLDTYPE=BIN,                        +03440000
               VALUE=XHDCXLV2,OPTS=MUTEXCL+FIXVALUE                     03450000
                                                                        03460000
         $XPTHDR SEQINDEP,XHDCSEQI,FLDTYPE=BOOL,VALUE=TRUE              03470000
         $XPTHDR PINGABLE,XHDCPING,FLDTYPE=BOOL,VALUE=TRUE              03480000
                                                                        03490000
         $XPTHDR FIRST,XHDCSEG,FLDTYPE=BIN,                            +03500000
               VALUE=XHDCS_F,OPTS=MUTEXCL+FIXVALUE                      03510000
         $XPTHDR MIDDLE,XHDCSEG,FLDTYPE=BIN,                           +03520000
               VALUE=XHDCS_M,OPTS=MUTEXCL+FIXVALUE                      03530000
         $XPTHDR LAST,XHDCSEG,FLDTYPE=BIN,                             +03540000
               VALUE=XHDCS_L,OPTS=MUTEXCL+FIXVALUE                      03550000
         $XPTHDR ONLY,XHDCSEG,FLDTYPE=BIN,                             +03560000
               VALUE=XHDCS_O,OPTS=MUTEXCL+FIXVALUE                      03570000
         $XPTHDR NONE,XHDCSEG,FLDTYPE=BIN,                             +03580000
               VALUE=XHDCS_N,OPTS=MUTEXCL+FIXVALUE                      03590000
                                                                        03600000
         EJECT ,                                                        03610000
