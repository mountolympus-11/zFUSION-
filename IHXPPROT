         $XPTHDR FIRST,XHLLSEG,FLDTYPE=BIN,                            +04140000
               VALUE=XHLLS_F,OPTS=MUTEXCL+FIXVALUE                      04150000
         $XPTHDR MIDDLE,XHLLSEG,FLDTYPE=BIN,                           +04160000
