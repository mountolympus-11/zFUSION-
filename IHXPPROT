         $XPTHDR ENHSQ_L3,XHLLSQSH,FLDTYPE=BOOL,VALUE=XHLL$XS3,        +04040000
               OPTS=FIXVALUE                                            04050000
         $XPTHDR ENHSQ_L1,XHLLSQSH,FLDTYPE=BOOL,VALUE=XHLL$XS1,        +04060000
