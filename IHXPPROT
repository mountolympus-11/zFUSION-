         $XPTHDR ONLY,XHLLSEG,FLDTYPE=BIN,                             +04200000
               VALUE=XHLLS_O,OPTS=MUTEXCL+FIXVALUE                      04210000
         $XPTHDR NONE,XHLLSEG,FLDTYPE=BIN,                             +04220000
               VALUE=XHLLS_N,OPTS=MUTEXCL+FIXVALUE                      04230000
